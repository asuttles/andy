// Example Factorial Program
int result, arg;

// Factorial Procedure
procedure fac;
   int n;

   begin
      n := arg;
      if n <= 1 then
         begin
            result := 1;
         end
      else
         begin
            arg := n - 1;
            call fac;
            result := n * result;
         end
   end;

// Main Program
begin
   arg := 5;
   call fac;
   write result;
end.